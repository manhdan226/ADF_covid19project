{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid-rp-adf"
		},
		"blob_covidrpsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blob_covidrpsa'"
		},
		"gen2_covidrpdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'gen2_covidrpdl'"
		},
		"gen2_covidrpdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidrpdl.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid-rp-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"blob_covidrpsa_connectionString": {
						"value": "[parameters('blob_covidrpsa_connectionString')]"
					},
					"gen2_covidrpdl_accountKey": {
						"value": "[parameters('gen2_covidrpdl_accountKey')]"
					},
					"gen2_covidrpdl_properties_typeProperties_url": {
						"value": "[parameters('gen2_covidrpdl_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}